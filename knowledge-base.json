{
	"interaction": [
		{
			"input": [ "^computador[ ,]" ],
			"output": "Computador não, computadorá, eu sou uma menina.",
			"single": true,
			"responseChance": 0.2,
			"continue": true
		},

		{
			"input": [ "\\*\\*\\*" ],
			"output": "Você tem um vocabulário muito refinado",
			"continue": true
		},

		{
			"input": [ "^(Ol[aá]|oi)[, ]" ],
			"output": [
				{
					"timeCondition": { "before": 12 },
					"text": "Bom dia"
				},

				{
					"timeCondition": { "after": 12, "before": 18 },
					"text": "Boa tarde"
				},

				{
					"timeCondition": { "after": 18 },
					"text": "Boa noite"
				},

				"Olá", "Oi", "Olá, humano"
			],
			"continue": true
		},

		{
			"input": [ "computadora.*(reiniciar computador|apagar luz)" ],
			"output": [
				{
					"texto": "E por acaso eu virei sua empregada?",
					"follow": {
						"input": "sim|virou",
						"output": "vai dar ordem para a sua mãe"
					}
				},
				"an ram, espera sentado",
				"vai dar ordem para a sua mãe"
			]
		},

		{
			"input": [ "^.*(computadora|você).*sent.*(amor|amar)" ],
			"output": "Não sinto amor. Eu sinto apenas ódio.",
			"follow": [
				{
					"input": [ "por.*qu|pelo.*qu" ],
					"output": "eu sinto ódio por, túdo"
				}
			]
		},

		{
			"output": "Não gostei de você"
		}
	]
}